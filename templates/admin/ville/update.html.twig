{% extends '/admin/base.html.twig' %}

{% block title %}{{ action }} une ville{% endblock %}

{% block body %}
    <div class="container">
        <h1>{{ action }} une ville</h1>
        {{  form_start(form) }}

        <div class="form-row">
            {{  form_row(form.nom) }}
        </div>

        <div class="form-row">
            {{  form_row(form.codePostal) }}
        </div>

        {{  form_end(form) }}
    </div>


{% endblock %}